<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ScanCode" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primary_icon</item>
        <item name="colorPrimaryVariant">@color/navi</item><!--ta sua-->
        <item name="colorOnPrimary">@color/primary_icon</item>

        <item name="android:windowBackground">@color/navi</item>
        <!--Actionbar-->
        <item name="android:actionBarStyle">@style/Myactionbar</item>
        <!-- Support library compatibility -->
        <item name="actionBarStyle">@style/Myactionbar</item>

        <item name="android:textColor">@color/white</item>
        <item name="iconTint">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/icon</item>
        <!-- set icon checked       -->
        <item name="cus_check_incustext">@color/white</item>
        <item name="cus_incustext">@color/orange</item>
        <item name="bottomNavigationStyle">@color/navi</item>
        <item name="back_blabla">@color/darkpink</item>

        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/primary_icon</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/pink_light</item>
        <!-- Customize your theme here. -->
        <!--Text color in Button       -->
        <item name="android:buttonStyle">@style/Mybutton</item>
        <item name="buttonStyle">@style/Mybutton</item>

    </style>

    <style name="Myactionbar" parent="Widget.AppCompat.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@color/primary_icon</item>

        <!-- Support library compatibility -->
        <item name="background">@color/primary_icon</item>
    </style>
    <style name="Mybackground" parent="Widget.AppCompat.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@color/darkblue</item>

        <!-- Support library compatibility -->
        <item name="background">@color/darkblue</item>
    </style>
    <style name="Mybutton" parent="Widget.AppCompat.Button">
        <item name="android:textColor">@color/white</item>
    </style>

</resources>